{
  "version": 2,
  "builds": [
    {
      "src": "Back/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "Front/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Front/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Back/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/Front/build/$1"
    }
  ]
}
