{
    "version": 2,
    "builds": [
      { 
        "src": "Back/index.js",   
        "use": "@vercel/node"   
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",  
        "dest": "Back/index.js"   
      },
      {
        "src": "/(.*)",   
        "dest": "Front/build/$1"  
      }
    ]
  }
  